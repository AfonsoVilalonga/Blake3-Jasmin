/*
    COMPLETE
*/
inline fn chunk_state_init_key(reg u64 self, stack u32[8] key_words, reg u64 chunk_counter, reg u32 flags){
    inline int i;
    for i = 0 to 8{
        (u32)[self + i * 4] = key_words[i];
    }   

    (u64)[self + 8 * 4] = chunk_counter;
    
    for i = 0 to 64{
        (u8)[self + 8 * 5 + i] = 0;
    }
    
    (u8)[self + 8 * 5 + 64] = 0;
    (u8)[self + 8 * 5 + 65] = 0;
    (u32)[self + 112 - 4] = flags;
}



/*
    COMPLETE
*/
inline fn hasher_init_internal(reg u64 self, stack u32[8] key_words, reg u32 flags){
    reg u64 chunck_counter;
    chunck_counter = 0;

    chunk_state_init_key(self, key_words, chunck_counter, flags);

    inline int i;
    for i = 0 to 8{
        (u32)[self + 112 + 4 * i] = key_words[i];
    }

    (u8)[self + 112 + 4 * 8 + (8 * 54) * 4] = 0;
    (u32)[self + 1880 - 4] = flags;

}



/*
    COMPLETE
*/
export fn blake3_hasher_init(reg u64 self){
    reg u32 flags;
    flags = 0;

    stack u32[8] IV; 
    IV[0] = 0x6A09E667;
    IV[1] = 0xBB67AE85;
    IV[2] = 0x3C6EF372;
    IV[3] = 0xA54FF53A;
    IV[4] = 0x510E527F;
    IV[5] = 0x9B05688C;
    IV[6] = 0x1F83D9AB;
    IV[7] = 0x5BE0CD19;

    hasher_init_internal(self, IV, flags);
}


/*
    COMPLETE
*/
inline fn words_from_little_endian_bytes_init(stack u8[32] key) -> stack u32[8]{
    stack u32[8] res;

    reg u32 aux;

    inline int i;
    inline int j;
    j = 0;

    for i = 0 to 8{
        aux = (32u) key[j];
        res[i] = aux;
        j += 1;
        
        aux = (32u) key[j];
        aux = aux << 8;
        res[i] += aux;
        j += 1;
        
        aux = (32u) key[j];
        aux = aux << 16;
        res[i] += aux;
        j += 1;
        
        aux = (32u) key[j];
        aux = aux << 24;
        res[i] += aux;
        j += 1;
    }

    return res;
}


/*
    COMPLETE
*/
export fn blake3_hasher_init_keyed(reg u64 self, reg u64 key){
    stack u8[32] key_aux;
    stack u32[8] key_words;
    
    reg u32 keyed_hash;
    
    keyed_hash = 1;
    keyed_hash = keyed_hash << 4;

    inline int i; 
    for i = 0 to 32{
        key_aux[i] = (u8)[key + i];
    }


    key_words = words_from_little_endian_bytes_init(key_aux);
    hasher_init_internal(self, key_words, keyed_hash);
}

