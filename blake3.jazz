/*
    Completa VERIFICAR
*/
inline fn hasher_push_stack(reg u64 self, stack u32[8] cv){
    inline int i;
    
    reg u64 index;
    
    index = (64u) (u8)[self + 112 + 8*4 + (8 * 54) * 4];
    index = index * 32;

    for i = 0 to 8{
        (u32)[self + index + 4 * i] = cv[i];
    }

    (u8)[self + 112 + 8*4 + (8 * 54) * 4] = (u8)[self + 112 + 8*4 + (8 * 54) * 4] + 1;
}


/*
    Completa VERIFICAR 
*/
inline fn parent_cv(reg u64 self, stack u32[8] right_child_cv) -> stack u32[8]{
    stack u32[8] out, input_chaining_value;
    stack u32[16] block_words;
    
    reg u64 counter;
    reg u32 block_len, flags;
    reg u8 aux;

    aux = (u8)[self + 112 + (8 * 4) + (8 * 54) * 4];
    input_chaining_value, block_words, counter, block_len, flags = parent_output(self, right_child_cv, aux);

    out = output_chaining_value(counter, block_len, flags, block_words, input_chaining_value);    

    return out;    
}



/*
    Completa VERIFICAR 
*/
inline fn hasher_add_chunk_cv(reg u64 self, stack u32[8] new_cv, reg u64 total_chunks){
    stack u32[8] out;
    reg u64 aux, condition;

    out = new_cv;

    condition = total_chunks;
    aux = condition; 
    aux = aux & 1;
    
    while(aux == 0){
        (u8)[self + 1872] = (u8)[self + 1872] - 1;

        out = parent_cv(self, out);    

        condition = condition >> 1;
        
        aux = condition; 
        aux = aux & 1;
    }
    hasher_push_stack(self, out);
}


/*
    COMPLETE VERIFICAR 
*/
inline fn chunk_state_init_update(reg u64 self, reg u64 chunck_counter){
    inline int i;
    for i = 0 to 8{
        (u32)[self + i * 4] = (u32)[self + 112 + i * 4];
    }   

    (u64)[self + 8 * 4] = chunck_counter;
    
    for i = 0 to 64{
        (u8)[self + 8 * 5 + i] = 0;
    }
    
    (u8)[self + 8 * 5 + 64] = 0;
    (u8)[self + 8 * 5 + 65] = 0;
    (u32)[self + 1880 - 4] = (u32)[self + 1880 - 4];
}


/* 
    COMPLETE VERIFICAR
*/
inline fn chunk_state_update(reg u64 self, reg u64 input, reg u64 input_len, reg u8 input_u8){
    stack u32[16] block_words, out16;
    stack u32[8] chaining_value;
    
    stack u8 input_u8_2;
    input_u8_2 = 0;
    
    reg u64 chunk_counter;
    reg u64 want;
    reg u64 take;
    reg u64 input_len_aux;
    reg u64 aux_64, aux_64_2, aux_64_3; 
    
    reg u32 flags;
    reg u32 BLAKE3_BLOCK_LEN;
    reg u32 aux_32; 

    reg u8 aux_8;
     

    BLAKE3_BLOCK_LEN = 64;    
    input_len_aux = input_len;
    

    inline int i;

    while(input_len_aux >s 0){
        
        if((u32)[self + 8 * 5 + 64] == 64){
            block_words = words_from_little_endian_bytes(self);
        
          
            for i = 0 to 8{
                chaining_value[i] = (u32)[self + 4 * i];
            }

            chunk_counter = (u64)[self + 4 * 8];
            aux_32 = chunk_state_start_flag(self);
            
            flags = (u32)[self + 108];
            flags = flags | aux_32;

            aux_64 = (u64)[self + 4 * i];
            out16 = compress(chaining_value, block_words, aux_64, BLAKE3_BLOCK_LEN, flags);

            for i = 0 to 8{
                (u32)[self + 4 * i] = out16[i]; 
            }

            aux_8 = (u8)[self + 5 * i + 64 + 1];
            aux_8 = aux_8 + 1;
            (u8)[self + 5 * 8 + 64 + 1] = aux_8;

            for i = 0 to 64{
                (u8)[self + 5 * 8 + i] = 0;
            }
            
            (u8)[self + 5 * 8 + 64] = 0;
        }

      
        aux_64 = (64u) (u8)[self + 5 * 8 + 64];
        want = 64;
        want = want - aux_64;
        take = want;

        if(want >s input_len_aux){
            take = input_len_aux;
        }


        aux_8 = input_u8_2;
        aux_8 = aux_8 + input_u8;

        aux_64 = 0;
        aux_64_3 = (64u) (u8)[self + 5 * 8 + 64];
        
        while(take >s aux_64){
            aux_64_2 = (64u) aux_8;
            aux_64_2 = aux_64_2 + aux_64;

            aux_64_3 = aux_64_3 + aux_64;
            
            (u8)[self + 5 * 8 + aux_64_3] = (u8)[input + aux_64_2];
            aux_64 = aux_64 + 1;
        }

        (u8)[self + 5 * 8 + 64] = (u8)[self + 5 * 8 + 64] + (8u) take;
        input_u8_2 = input_u8_2 + 1;
        input_len_aux = input_len_aux - take;
    }
}


/* 
    COMPLETE VERIFICAR
*/
export fn blake3_hasher_update_aux(reg u64 self, reg u64 input, reg u64 input_len){
    reg u64 res, total_chunks;
    
    reg u64 counter;
    reg u32 block_len, flags;
    stack u32[16] block_words;
    stack u32[8] input_chaining_value;  
    stack u32[8] chunk_cv;

    reg u64 want, take;
    reg u8 input_u8;
    input_u8 = 0;
    
    while(input_len >s 0){

        res = chunk_state_len(self);

        if(res == 1024){
            counter, block_len, flags, block_words, input_chaining_value = chunk_state_output(self);
            chunk_cv = output_chaining_value(counter, block_len, flags, block_words, input_chaining_value);

            total_chunks = (u64)[self + 4 * 8];
            total_chunks = total_chunks + 1;

            hasher_add_chunk_cv(self, chunk_cv, total_chunks);
            
            chunk_state_init_update(self, total_chunks);
        }
        
        want = 1024;
        res = chunk_state_len(self);
        want = want - res;
        
        take = want;

        if(want >s input_len){
            take = input_len;
        }
        
        chunk_state_update(self, input, take, input_u8);
        input_u8 = input_u8 + take;
        input_len = input_len - take;

    }

}






